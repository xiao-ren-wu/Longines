<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.longines.dao.OrderMapper">
    <resultMap id="ddd" type="com.longines.pojo.Order">
        <id column="o_id" property="o_id"/>
        <result column="g_id" property="g_id"/>
        <result column="g_num" property="g_num"/>
    </resultMap>
    <select id="findOrderById" resultMap="ddd" parameterType="java.lang.Integer">
        SELECT * FROM tb_order WHERE o_id=#{o_id}
    </select>

</mapper>